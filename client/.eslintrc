{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2019,
		"sourceType": "module",
		"jsx": true
	},
	"extends": [
		"xo-react/space",
		"xo-space/esnext",
		"xo-space/browser",
		"plugin:unicorn/recommended"
	],
	"rules": {
		"object-curly-spacing": ["error", "always"],
		"comma-dangle": ["error", "always-multiline"],
		"react/prop-types": "none"
	}
}